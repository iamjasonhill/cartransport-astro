---
import BaseLayout from '../layouts/BaseLayout.astro';
import Section from '../components/Section.astro';

const title = 'Vehicle Transport Interstate from Adelaide to Emerald. | Car Transport by Moving Again';
const description = "Moving your car from Adelaide to Emerald? Let Moving Cars help you move your car. We've been transporting cars around Australia for over 15 years. Give us a call and we'll provide an instant price.";
// ogImage, canonicalUrl, and other meta should be handled in the layout if needed

---

<BaseLayout title={title} description={description}>
  <Section name="Hero" class="relative bg-gradient-to-r from-blue-600 to-blue-800 text-white py-20 md:py-32 overflow-hidden">
    <div class="absolute inset-0">
      <img 
        src="/images/hero-placeholder.svg" 
        alt="" 
        class="w-full h-full object-cover"
        decoding="async"
        loading="eager"
        width="1920"
        height="1080"
        style="object-position: center center;"
      />
    </div>
    <div class="container mx-auto px-4 relative z-10">
      <div class="max-w-3xl mx-auto text-center hero-content">
        <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold mb-6 leading-tight" 
            style="will-change: transform, opacity;">
          Vehicle Transport Interstate from Adelaide to Emerald
        </h1>
        <p class="text-xl md:text-2xl mb-8 text-blue-100 max-w-2xl mx-auto">
          Safe, reliable, and affordable car transport between Adelaide and Emerald. Get a free quote today!
        </p>
        <div class="flex flex-col sm:flex-row justify-center gap-4">
          <a 
            href="https://vehicles.movingagain.com.au/quote/multi-step?mtm_campaign=Standard_Vehicle_Quote&mtm_kwd=MACT" 
            class="bg-white text-blue-700 hover:bg-blue-50 px-8 py-4 rounded-md font-bold text-lg transition-colors text-center"
          >
            Standard Car Transport Quote
          </a>
          <a 
            href="https://vehicles.movingagain.com.au/quote-types?mtm_campaign=Non_Standard_Vehicle_Quote&mtm_kwd=MACT" 
            class="bg-transparent border-2 border-white text-white hover:bg-white/10 px-8 py-4 rounded-md font-bold text-lg transition-colors text-center"
          >
            Non-Standard Vehicle Quote
          </a>
        </div>
      </div>
    </div>
  </Section>
  <section id="main1" class="mb-10 container mx-auto px-4 py-8">
    <h2 class="text-xl font-bold text-blue-700 mb-4">Main Section 1</h2>
    <!-- Main Content Section 1 -->
  </section>
  <Section name="Features" class="py-16 bg-gradient-to-br from-gray-50 to-white">
    <div class="container mx-auto px-4">
      <div class="text-center max-w-3xl mx-auto mb-16">
        <span class="inline-block text-blue-600 font-semibold mb-3">WHY CHOOSE US</span>
        <h2 class="text-3xl md:text-4xl font-bold mb-4">Your Trusted Car Transport from South Australia to Queensland</h2>
        <div class="w-24 h-1 bg-blue-600 mx-auto"></div>
      </div>
      <div class="grid grid-cols-1 md:grid-cols-3 gap-8 max-w-6xl mx-auto">
        <!-- Feature 1 -->
        <div class="group bg-white p-8 rounded-xl shadow-sm hover:shadow-xl transition-all duration-300 border border-gray-100 hover:border-blue-100">
          <div class="w-16 h-16 bg-blue-50 rounded-xl flex items-center justify-center mb-6 mx-auto group-hover:bg-blue-100 group-hover:scale-110 transition-all duration-300">
            <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3 text-center text-gray-900">South Australia to Queensland Experts</h3>
          <p class="text-gray-600 text-center leading-relaxed">
            Decades of experience moving vehicles safely between SA and QLD, with local knowledge at both ends.
          </p>
        </div>
        <!-- Feature 2 -->
        <div class="group bg-white p-8 rounded-xl shadow-sm hover:shadow-xl transition-all duration-300 border border-gray-100 hover:border-blue-100">
          <div class="w-16 h-16 bg-blue-50 rounded-xl flex items-center justify-center mb-6 mx-auto group-hover:bg-blue-100 group-hover:scale-110 transition-all duration-300">
            <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3 text-center text-gray-900">Depot & Door-to-Door Options</h3>
          <p class="text-gray-600 text-center leading-relaxed">
            Choose the convenience of depot drop-off or door-to-door delivery anywhere in South Australia or Queensland.
          </p>
        </div>
        <!-- Feature 3 -->
        <div class="group bg-white p-8 rounded-xl shadow-sm hover:shadow-xl transition-all duration-300 border border-gray-100 hover:border-blue-100">
          <div class="w-16 h-16 bg-blue-50 rounded-xl flex items-center justify-center mb-6 mx-auto group-hover:bg-blue-100 group-hover:scale-110 transition-all duration-300">
            <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3 text-center text-gray-900">Fast Quotes & No Surprises</h3>
          <p class="text-gray-600 text-center leading-relaxed">
            Get a quick, accurate quote and enjoy transparent pricingâ€”no hidden fees, just reliable service.
          </p>
        </div>
      </div>
    </div>
  </Section>
  <section id="main2" class="mb-10">
    <h2 class="text-xl font-bold text-blue-700 mb-4">Main Section 2</h2>
    <!-- Main Content Section 2 -->
  </section>
  <Section name="Popular Routes" class="py-16 bg-gray-50">
    <div class="container mx-auto px-4">
      <div class="text-center mb-12">
        <h2 class="text-3xl md:text-4xl font-bold mb-4">Popular Transport Routes</h2>
        <p class="text-gray-600 max-w-2xl mx-auto">
          We provide reliable car transport services between all major Australian cities and regional areas.
        </p>
      </div>
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        <!-- Route 1 -->
        <a href="/sydney-to-melbourne" class="group block bg-white rounded-lg shadow-md overflow-hidden hover:shadow-lg transition-shadow">
          <div class="h-48 bg-gray-200 relative overflow-hidden">
            <div class="absolute inset-0 bg-gradient-to-t from-black/60 to-transparent z-10"></div>
            <div class="w-full h-full bg-gradient-to-br from-blue-600 to-blue-800 flex items-center justify-center text-white text-lg font-medium">
              Sydney to Melbourne
            </div>
            <h3 class="absolute bottom-0 left-0 right-0 text-white text-xl font-bold p-4 z-20">
              Sydney to Melbourne
            </h3>
          </div>
          <div class="p-4">
            <p class="text-gray-600">From $599</p>
            <div class="flex items-center text-blue-600 mt-2">
              <span>View Details</span>
              <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
            </div>
          </div>
        </a>
        <!-- Route 2 -->
        <a href="/brisbane-to-sydney" class="group block bg-white rounded-lg shadow-md overflow-hidden hover:shadow-lg transition-shadow">
          <div class="h-48 bg-gray-200 relative overflow-hidden">
            <div class="absolute inset-0 bg-gradient-to-t from-black/60 to-transparent z-10"></div>
            <div class="w-full h-full bg-gradient-to-br from-blue-600 to-blue-800 flex items-center justify-center text-white text-lg font-medium">
              Brisbane to Sydney
            </div>
            <h3 class="absolute bottom-0 left-0 right-0 text-white text-xl font-bold p-4 z-20">
              Brisbane to Sydney
            </h3>
          </div>
          <div class="p-4">
            <p class="text-gray-600">From $699</p>
            <div class="flex items-center text-blue-600 mt-2">
              <span>View Details</span>
              <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
            </div>
          </div>
        </a>
        <!-- Route 3 -->
        <a href="/canberra-to-melbourne" class="group block bg-white rounded-lg shadow-md overflow-hidden hover:shadow-lg transition-shadow">
          <div class="h-48 bg-gray-200 relative overflow-hidden">
            <div class="absolute inset-0 bg-gradient-to-t from-black/60 to-transparent z-10"></div>
            <div class="w-full h-full bg-gradient-to-br from-blue-600 to-blue-800 flex items-center justify-center text-white text-lg font-medium">
              Canberra to Melbourne
            </div>
            <h3 class="absolute bottom-0 left-0 right-0 text-white text-xl font-bold p-4 z-20">
              Canberra to Melbourne
            </h3>
          </div>
          <div class="p-4">
            <p class="text-gray-600">From $499</p>
            <div class="flex items-center text-blue-600 mt-2">
              <span>View Details</span>
              <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
              </svg>
            </div>
          </div>
        </a>
      </div>
      <div class="text-center mt-10">
        <a href="/areas" class="inline-block bg-blue-600 hover:bg-blue-700 text-white font-bold py-3 px-8 rounded-md transition-colors">
          View All Routes
        </a>
      </div>
    </div>
  </Section>
  <section id="main3" class="mb-10">
    <h2 class="text-xl font-bold text-blue-700 mb-4">Main Section 3</h2>
    <!-- Main Content Section 3 -->
  </section>
  <section>
    <h2>We offer a number of different services to our customers:</h2>
    <h3>Depot to Depot transport service:</h3>
    <p>We provide Depot to Depot services in almost all areas of Australia. This gives you the option to deliver the vehicle at a time convenient to you, rather than waiting for us to come and collect from you.</p>
    <h3>Door to Door transport service:</h3>
    <p>Emerald</p>
    <p>We also provide either Door to Depot, or Depot to Door services as well.</p>
    <h3>Non-drivable Transport:</h3>
    <p>We're also able to supply the service of transporting non drivable vehicles for our clients.</p>
    <p>There are some restrictions regarding this, if this is something you need then head over to our non-drivable quote page to secure a price. <a href="https://transportnondrivablecars.com.au/" target="_blank" rel="noopener">https://transportnondrivablecars.com.au/</a></p>
    <h3>Enclosed Vehicle Transport:</h3>
    <p>This is generally used by those people with vintage/high value vehicles, vehicles transported through this service are completely enclosed for all transport from depot to depot. (If you choose a door to door service they are not enclosed from when they reach our depot to when they are delivered to your address)</p>
    <p><b>We have numerous depots across the country.</b></p>
    <p>One of the reasons we are able to supply our highly competitive prices across the country is the fact that we have depot locations in all major cities around Australia. This allows us to perform our services very regularly throughout Australia.</p>
  </section>
  <section>
    <h2>What does all this mean for you?</h2>
    <p>On almost any given day, all quotes are sent back to you within an hour, however more often than not a quotation will come back within 10 to 15 minutes, if not immediately.</p>
    <p>Also note that we are able to supply quotations on any day of the week, so if you're free during the weekend looking to transport a car, you may get your response back within a few minutes of submitting it at any point in the week.</p>
  </section>
  <section>
    <h2>Want to know why we're the best?</h2>
    <p>Read some reviews of our service from previous customers. We don't just claim to be an excellent choice in transporting cars from Adelaide to Emerald, people tell us we are.</p>
    <p>Previous customer testimonies. <a href="https://vehicles.movingagain.com.au/">View</a></p>
  </section>
  <section>
    <h2>Are you in need of getting your vehicle from Adelaide to Emerald as fast as possible?</h2>
    <p>We are always striving to offer our customers with a faster and safer journey. Transporting a car from Adelaide to Emerald is quick, simple and easy with us. We're aware that our clients rely on their cars for a number of reasons so strive to get them to you as quick as we can. We offer a number of services varying from the cheapest, to the quickest available.</p>
  </section>
  <section>
    <h2>Book with a company that you can place your trust in.</h2>
    <p>Moving Again has been Moving Again from Adelaide to Emerald for over 25 years. We've been involved in this industry for longer than almost anyone else. Would you really trust your vehicle in the hands of an inexperienced vehicle moving company? How could you risk the damage that could incur by using an amateur transport company to move your car for you.</p>
    <p>The premium level of service and care we provide each car relocated through Moving Again has turned us into one of the industry leaders for moving vehicles across Australia. We strive to provide a top level of care along with prompt responses to any concerns or questions. We take a great deal of pride in the service we provide our customers. We do all this whilst ensuring that we are always highly competitive and an affordable choice to move your car from Adelaide to Emerald.</p>
  </section>
  <section>
    <h2>We're in Emerald soon!</h2>
    <p>We are booked in to be making a delivery to Emerald very soon. Below is where we're going, you might be lucky enough to catch our team in action!</p>
    <iframe
      width="100%"
      height="250"
      style="border:0"
      loading="lazy"
      allowfullscreen
      referrerpolicy="no-referrer-when-downgrade"
      src="https://www.google.com/maps?q=Emerald,+Australia&output=embed">
    </iframe>
  </section>
  <section>
    <h2>We make Moving Cars easy.</h2>
    <p>As we've said before, we've been doing this for quite some time. So throughout the years we have strived to make the process as easy and fast as we can. Which begins with the fact that we provide instant quotes to our customers for a very large portion of the country. When it comes to actually booking in the move, we provide you a simple, easy to use online booking option. Alternatively you're able to give us a call, answer just a few questions, and your car is ready to move. We do everything we can to ensure transporting your vehicle from Adelaide to Emerald is as easy as possible.</p>
  </section>
  <section>
    <h2>Book a cheap, yet convenient car transport service.</h2>
    <p>Whilst we can safely say we are across the board one of the cheapest locations for vehicle transport, that does not mean we do so by sacrificing quality or convenience.</p>
    <p>Our depots are open from 8:30am to 4:00pm Monday to Friday, and allow you to drop your vehicle off anytime in-between, any day of the week.</p>
    <p>If you need to drop your vehicle off at the depot we can arrange that to happen tomorrow, if you need it to be collected we just need two days notice. On top of all that, we are proud to be an Australian owned, family run business, what more reason do you need?</p>
    <p class="carry">To always be certain you are getting the cheapest price possible, we match your requirements with the best car carrier. Car transport from Adelaide to Emerald is what we do best!</p>
  </section>
  <section>
    <h2>Ready for the best quote you'll find?</h2>
    <p>Then please follow the link to get an obligation <a href="https://vehicles.movingagain.com.au/">free quote now</a>.</p>
  </section>
  <aside class="sidebar">
    <h2>Transporting Vehicles</h2>
    <p>Need to ship that special car interstate, across the country? No matter the vehicle type or transport requirements, Moving Again can meet your car transport service and delivery details with ease.</p>
  </aside>
  <Section name="How It Works" class="py-16 bg-white">
    <div class="container mx-auto px-4">
      <div class="text-center mb-12">
        <h2 class="text-3xl md:text-4xl font-bold mb-4">How It Works</h2>
        <div class="w-24 h-1 bg-blue-600 mx-auto"></div>
      </div>
      <div class="grid grid-cols-1 md:grid-cols-3 gap-8 max-w-5xl mx-auto">
        <!-- Step 1 -->
        <div class="text-center">
          <div class="w-20 h-20 bg-blue-100 rounded-full flex items-center justify-center mx-auto mb-4 relative">
            <span class="text-3xl font-bold text-blue-600">1</span>
          </div>
          <h3 class="text-xl font-bold mb-2">Get a Free Adelaide to Emerald Quote</h3>
          <p class="text-gray-600">
            Instantly receive a quote for your car transport from South Australia to Queensland.
          </p>
        </div>
        <!-- Step 2 -->
        <div class="text-center">
          <div class="w-20 h-20 bg-blue-100 rounded-full flex items-center justify-center mx-auto mb-4 relative">
            <span class="text-3xl font-bold text-blue-600">2</span>
          </div>
          <h3 class="text-xl font-bold mb-2">Book Your Preferred Date</h3>
          <p class="text-gray-600">
            Secure your spot for a seamless Adelaide to Emerald vehicle move.
          </p>
        </div>
        <!-- Step 3 -->
        <div class="text-center">
          <div class="w-20 h-20 bg-blue-100 rounded-full flex items-center justify-center mx-auto mb-4 relative">
            <span class="text-3xl font-bold text-blue-600">3</span>
          </div>
          <h3 class="text-xl font-bold mb-2">We Handle the Rest</h3>
          <p class="text-gray-600">
            Our team collects and delivers your car safely between SA and QLD.
          </p>
        </div>
      </div>
    </div>
  </Section>
  <Section name="Call to Action" class="py-16 bg-blue-700 text-white">
    <div class="container mx-auto px-4 text-center">
      <h2 class="text-3xl md:text-4xl font-bold mb-6">Ready to Move Your Car from Adelaide to Emerald?</h2>
      <p class="text-xl mb-8 max-w-2xl mx-auto text-blue-100">
        Get a free, no-obligation quote for your Adelaide to Emerald car transport and experience the Moving Again difference.
      </p>
      <div class="flex flex-col sm:flex-row justify-center gap-4 max-w-3xl mx-auto">
        <a 
          href="https://vehicles.movingagain.com.au/quote/multi-step?mtm_campaign=Standard_Vehicle_Quote&mtm_kwd=MACT" 
          class="bg-white text-blue-700 hover:bg-blue-50 px-6 py-4 rounded-md font-bold text-lg transition-colors text-center flex-1"
        >
          Standard Car Transport Quote
        </a>
        <a 
          href="https://vehicles.movingagain.com.au/quote-types?mtm_campaign=Non_Standard_Vehicle_Quote&mtm_kwd=MACT" 
          class="bg-transparent border-2 border-white text-white hover:bg-white/10 px-6 py-4 rounded-md font-bold text-lg transition-colors text-center flex-1"
        >
          Non-Standard Vehicle Quote
        </a>
      </div>
    </div>
  </Section>
</BaseLayout> 